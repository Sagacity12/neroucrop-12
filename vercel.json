{
  "version": 2,
  "builds": [
    {
      "src": "src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/auth/google",
      "dest": "/src/main.js"
    },
    {
      "src": "/api/v1/auth/google/callback",
      "dest": "/src/main.js"
    },
    {
      "src": "/api/v1/auth/(.*)",
      "dest": "/src/main.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/main.js"
    }
  ]
} 